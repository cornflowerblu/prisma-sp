type Client {
    id: ID! @id
    clientId: String! @unique
    name: String!
    dba: String
    address: String!
    city: String!
    stateRef: State! @relation(link: INLINE)
    zipCode: String!
    positions: [Position!]! @relation(name: "ClientPositions")
}

type Position {
    id: ID! @id
    code: ID! @unique
    name: String!
    clientRef: [Client!]! @relation(name: "ClientPositions")
}